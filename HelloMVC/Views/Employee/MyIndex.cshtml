@model List<HelloMVC.Models.Employee>
@{
    ViewBag.Title = "View";
    //Layout = null;
}

<h2>EmployeeIndex</h2>

<table class=" table">
    <tr>
        <td>ID</td>
        <td>Firstname</td>
        <td>Lastname</td>
        <td>DOB</td>
    </tr>
    @foreach (var employee in Model)
    {
    <tr>
        <td>@employee.Id</td>
        <td>@employee.Firstname</td>
        <td>@employee.Lastname</td>
        <td>@employee.DOB</td>

    </tr>
    }
</table>